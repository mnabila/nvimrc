{
  "todo": {
    "prefix": ["todo"],
    "body": ["TODO(${1:Name}): ${0:Summary}"],
    "description": "TODO reminder"
  },
  "fixme": {
    "prefix": ["fixme"],
    "body": [
      "FIXME(${VIM:system('git config github.user | tr -d \"\n\"')}): ${0:Summary}"
    ],
    "description": "FIXME reminder"
  },
  "pwgen": {
    "prefix": ["pwgen"],
    "body": ["${VIM:system('pwgen -s 32 -1 | tr -d \"\n\"')}"],
    "description": "Generate password"
  }
}
