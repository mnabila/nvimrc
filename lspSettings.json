{
  "pyls": {
    "plugins": {
      "jedi_completion": {
        "include_params": false
      },
      "autopep8": {
        "enabled": false
      },
      "mccabe": {
        "enabled": false
      },
      "pycodestyle": {
        "enabled": true,
        "ignore": ["E402", "C0103"]
      },
      "pydocstyle": {
        "enabled": false
      },
      "pyflakes": {
        "enabled": false
      },
      "rope": {
        "enabled": false
      },
      "yapf": {
        "enabled": true
      },
      "pylint": {
        "enabled": false
      }
    }
  },
  "cquery": {
    "snippetSupport": true
  },
  "http": {
    "proxy": "",
    "proxyStrictSSL": true
  },
  "json": {
    "format": {
      "enable": true
    },
    "schemas": [
      {
        "url": "http://json.schemastore.org/config",
        "schema": {
          "type": "array"
        }
      }
    ]
  }
}
